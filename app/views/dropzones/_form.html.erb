<div id = "create_edit">
  <div id="events_create_edit">
    <%= form_for(@dropzone, :html => {:multipart => true}) do |f| %>
      <%= f.label :state %>
      <%= f.text_field :state, :required => true, :autofocus => true %>
      <%= f.label :dz_name %>
      <%= f.text_field :dz_name, :required => true %>
      <%= f.label :image %>
      <%= f.cl_image_upload :image %>
      <%= f.label :latitude %>
      <%= f.number_field :latitude, step: :any %>
      <%= f.label :longitude %>
      <%= f.number_field :longitude, step: :any%>
      <%= f.submit %>
    <% end %>
  </div>
</div>
